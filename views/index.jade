extends layout

block content  
  h1  We'll be coming soon
  button(id="join") Join us
 
  ul
    for startup in startups
      for startupIn in startup
        h1=startupIn.title
        a(href="http://"+startupIn.body.url, target="_blank") hello     
        h3=startupIn.body.founders
        h3=startupIn.body.city

    else
      li sorry, no startups!